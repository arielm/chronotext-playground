OTHER_LDFLAGS[sdk=iphonesimulator*][arch=*] = $(BASE_OTHER_LDFLAGS) "$(CINDER_PATH)/lib/libcinder-iphone-sim_d.a" "$(CINDER_PATH)/blocks/FMOD/lib/ios/libfmodL_iphonesimulator.a"
OTHER_LDFLAGS[sdk=iphoneos*][arch=*] = $(BASE_OTHER_LDFLAGS) "$(CINDER_PATH)/lib/libcinder-iphone_d.a" "$(CINDER_PATH)/blocks/FMOD/lib/ios/libfmodL_iphoneos.a"

GCC_PREPROCESSOR_DEFINITIONS = DEBUG=1
